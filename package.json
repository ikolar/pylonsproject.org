    "babel-loader": "^6.2.4",
    "css-loader": "^0.26.1",
    "ejs": "^2.4.2",
    "ejs-compiled-loader": "^2.1.1",
    "expose-loader": "^0.7.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
